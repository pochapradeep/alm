	<build>
		<finalName>almAdapterService</finalName>

		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.3.1</version>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<profiles>
		<profile>
			<id>jira-7.2</id>
			<activation>
				<activeByDefault>false</activeByDefault>
			</activation>
			<properties>
				<jira.version>7.2.0-SNAPSHOT</jira.version>
				<jira.test-lib.version>${jira.version}</jira.test-lib.version>
				<amps.version>5.1.18</amps.version>
				<testkit.version>7.2.0</testkit.version>
			</properties>
			<dependencies>
<!-- 				<dependency>
					<groupId>com.atlassian.jira</groupId>
					<artifactId>jira-func-tests-legacy</artifactId>
					<version>${jira.test-lib.version}</version>
				</dependency> -->
			</dependencies>
		</profile>
	</profiles>

</project>